# Trading AI - Environment Variables
# Copy this file to .env and fill in your values
# NEVER commit .env to git!

# Binance API
BINANCE_API_KEY=
BINANCE_API_SECRET=

# Telegram Bot
TELEGRAM_TOKEN=
TELEGRAM_CHAT_ID=

# FTMO Account
FTMO_LOGIN=
FTMO_PASS=
FTMO_SERVER=

# Database
DB_URL=postgresql+psycopg://user:pass@localhost:5432/tradingai

# Confidence & Thresholds
CONFIDENCE_THRESHOLD=0.75
ENSEMBLE_WEIGHTS=0.35,0.40,0.25

# Safety Rails
KILL_SWITCH=false
MAX_SIGNALS_PER_HOUR=10
MAX_SIGNALS_PER_HOUR_HIGH=5

# Latency Budget
LATENCY_BUDGET_MS=500

# Model Configuration
MODEL_VERSION=v1.0.0
MODEL_PATH=models/promoted/

# Logging
LOG_LEVEL=INFO
LOG_FORMAT=json
